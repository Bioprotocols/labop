@prefix : <http://bioprotocols.org/paml#> .
@prefix opil: <http://bioprotocols.org/opil/v1#> .
@prefix om: <http://www.ontology-of-units-of-measure.org/resource/om-2/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix paml: <http://bioprotocols.org/paml#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix sbol: <http://sbols.org/v3#> .
@prefix uml: <http://bioprotocols.org/uml#> .
@prefix MathM: <http://www.w3.org/1998/Math/MathML> .
@prefix prov: <http://www.w3.org/ns/prov#> .
@base <http://bioprotocols.org/paml#> .

<http://bioprotocols.org/paml> rdf:type owl:Ontology ;
                              owl:imports uml:, sbol: , om: ;
                              rdfs:comment "Protocol Activity Modeling Languge (PAML) ontology." ;
                              owl:versionInfo "0.3" .

#################################################################
#    Annotation properties
#################################################################

###  http://www.w3.org/2002/07/owl#maxCardinality
owl:maxCardinality rdf:type owl:AnnotationProperty .
owl:minCardinality rdf:type owl:AnnotationProperty .


#################################################################
#    Datatypes
#################################################################

###  http://www.w3.org/2001/XMLSchema#anySimpleType
xsd:anySimpleType rdf:type rdfs:Datatype .


#################################################################
#    Protocol definitions, based on UML
#################################################################

paml:Protocol rdf:type owl:Class ;
              rdfs:comment "A protocol is an activity in the laboratory" ;
        rdfs:subClassOf uml:Activity .


paml:Primitive rdf:type owl:Class ;
              rdfs:comment "A behavior that is a base-level library function" ;
        rdfs:subClassOf uml:Behavior .


#################################################################
#    Execution record, bootstrapping from PROV-O
#################################################################

paml:Execution rdf:type owl:Class ;
        rdfs:comment "An execution is a record of the values produced by the operation of a protocol, real or simulated" ;
        rdfs:subClassOf sbol:TopLevel .


#################################################################
#    Sample collection data model
#################################################################

paml:SampleCollection rdf:type owl:Class ;
        rdfs:comment "A sample collection is a group of samples" ;
        rdfs:subClassOf sbol:Identified .


paml:SampleArray rdf:type owl:Class ;
        rdfs:comment "A sample array is an n-dimensional rectangular group of samples, either with specified contents or empty, all stored in the same type of container" ;
        rdfs:subClassOf paml:SampleCollection ,
            [ rdf:type owl:Restriction ;
              owl:onProperty paml:containerType ;
              owl:allValuesFrom xsd:anyURI ; # replace with container ontology, when available
              owl:minCardinality "1"^^xsd:nonNegativeInteger ;
              owl:maxCardinality "1"^^xsd:nonNegativeInteger ;
              rdfs:comment "Type of container used for storing the samples. The size and dimension may not match that of the array: it is up to execution to lay out the array in one or more containers"
            ] ,
            [ rdf:type owl:Restriction ;
              owl:onProperty paml:contents ;
              owl:allValuesFrom xsd:string ; # need to replace this with an serializable array model
              owl:minCardinality "1"^^xsd:nonNegativeInteger ;
              owl:maxCardinality "1"^^xsd:nonNegativeInteger ;
              rdfs:comment "N-dimensional array of URI for specification or nulls"
            ] .

paml:containerType rdf:type owl:DatatypeProperty ;
              rdfs:domain paml:SampleArray ;
              rdfs:range xsd:anyURI ;
              rdfs:label "container_type" .

paml:contents rdf:type owl:DatatypeProperty ;
              rdfs:domain paml:SampleArray ;
              rdfs:range xsd:string ;
              rdfs:label "contents" .

paml:SampleMask rdf:type owl:Class ;
        rdfs:comment "A sample mask is a subset of a SampleCollection, as defined by an array of boolean indiating whether each contents value is included or not" ;
        rdfs:subClassOf paml:SampleCollection ,
            [ rdf:type owl:Restriction ;
              owl:onProperty paml:source ;
              owl:allValuesFrom paml:SampleCollection ;
              owl:minCardinality "1"^^xsd:nonNegativeInteger ;
              owl:maxCardinality "1"^^xsd:nonNegativeInteger ;
              rdfs:comment "Collection being subsetted via mask"
            ] ,
            [ rdf:type owl:Restriction ;
              owl:onProperty paml:mask ;
              owl:allValuesFrom xsd:string ; # need to replace this with an serializable array model
              owl:minCardinality "1"^^xsd:nonNegativeInteger ;
              owl:maxCardinality "1"^^xsd:nonNegativeInteger ;
              rdfs:comment "N-dimensional array of booleans indicating whether contents are included in mask or not"
            ] .

paml:source rdf:type owl:ObjectProperty ;
              rdfs:domain paml:SampleMask ;
              rdfs:range paml:SampleCollection ;
              rdfs:label "source" .

paml:mask rdf:type owl:DatatypeProperty ;
              rdfs:domain paml:mask ;
              rdfs:range xsd:string ;
              rdfs:label "mask" .




paml:SampleData rdf:type owl:Class ;
              rdfs:comment "Collection of samples with data at locations" ;
            rdfs:subClassOf sbol:Identified ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty paml:sampleDataValues ;
                              owl:allValuesFrom xsd:string ; # need to replace this with an serializable array model
                              owl:minCardinality "1"^^xsd:nonNegativeInteger ;
                              owl:maxCardinality "1"^^xsd:nonNegativeInteger ;
                              ] ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty paml:fromSamples ;
                              owl:allValuesFrom paml:SampleCollection ;
                              owl:minCardinality "1"^^xsd:nonNegativeInteger ;
                              owl:maxCardinality "1"^^xsd:nonNegativeInteger ;
                              ] .

paml:sampleDataValues rdf:type owl:ObjectProperty ;
              rdfs:domain paml:SampleData ;
              rdfs:range xsd:string ; # TODO: consider https://github.com/SynBioDex/sbol_factory/issues/7
              rdfs:label "values" .

paml:fromSamples rdf:type owl:ObjectProperty ;
              rdfs:subPropertyOf opil:compositionalProperty ;
              rdfs:domain paml:SampleData ;
              rdfs:range paml:SampleCollection ;
              rdfs:label "from_samples" .

#################################################################
#    Time properties
#################################################################

# <timeproperty> ::= prov:startedAtTime | prov:endedAtTime
# <timevariable> ::= <URI for Protocol, Activity, ...> valueOf <timeproperty>
# <duration> ::= Difference(<timevariable>, <timevariable>) | OM:Measure

paml:timeVariableValue rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf opil:compositionalProperty ;
                rdfs:comment "Time value" ;
                rdfs:label "value" ;
                rdfs:domain paml:TimeVariable ;
                rdfs:range sbol:Measure .

paml:timeProperty rdf:type owl:DatatypeProperty ;
                rdfs:comment "Time variable type in relation to an activity" ;
                rdfs:label "time_property" ;
                rdfs:domain paml:TimeVariable ;
                rdfs:range xsd:anyURI .

paml:timeOf rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf opil:compositionalProperty ;
                rdfs:comment "Time variable type in relation to an activity" ;
                rdfs:label "time_of" ;
                rdfs:domain paml:TimeVariable ;
                rdfs:domain paml:Duration ;
                rdfs:range paml:Control ;
                rdfs:range paml:Executable .

paml:TimeVariable rdf:type owl:Class ;
          rdfs:comment "The value of a timevariable" ;
          rdfs:subClassOf sbol:TopLevel ,
          [ rdf:type owl:Restriction ;
                  owl:onProperty paml:timeProperty ;
                  owl:minCardinality "0"^^xsd:nonNegativeInteger ;
                  owl:maxCardinality "1"^^xsd:nonNegativeInteger ;
          ] ,
          [ rdf:type owl:Restriction ;
                  owl:onProperty paml:timeOf ;
                  owl:minCardinality "0"^^xsd:nonNegativeInteger ;
                  owl:maxCardinality "1"^^xsd:nonNegativeInteger ;
          ] ,
          [ rdf:type owl:Restriction ;
                  owl:onProperty paml:timeVariableValue ;
                  owl:onClass sbol:Measure ;
                  owl:minCardinality "0"^^xsd:nonNegativeInteger ;
                  owl:maxCardinality "1"^^xsd:nonNegativeInteger ;
          ].

paml:differenceSource rdf:type owl:ObjectProperty ;
                        rdfs:subPropertyOf opil:compositionalProperty ;
                        rdfs:comment "Difference has a source" ;
                        rdfs:label "source" ;
                        rdfs:domain paml:Difference ;
                        rdfs:range paml:TimeVariable .

paml:differenceDestination rdf:type owl:ObjectProperty ;
                        rdfs:subPropertyOf opil:compositionalProperty ;
                        rdfs:comment "Difference has a destination";
                        rdfs:label "destination" ;
                        rdfs:domain paml:Difference ;
                        rdfs:range paml:TimeVariable .

paml:Difference rdf:type owl:Class ;
                rdfs:comment "Difference is the dest - src values" ;
                rdfs:subClassOf sbol:TopLevel ,
                [ rdf:type owl:Restriction ;
                  owl:onProperty paml:differenceSource ;
                  owl:onClass paml:TimeVariable ;
                  owl:minCardinality "1"^^xsd:nonNegativeInteger ;
                  owl:maxCardinality "1"^^xsd:nonNegativeInteger ;
                ] ,
                [ rdf:type owl:Restriction ;
                  owl:onProperty paml:differenceDestination ;
                  owl:onClass paml:TimeVariable ;
                  owl:minCardinality "1"^^xsd:nonNegativeInteger ;
                  owl:maxCardinality "1"^^xsd:nonNegativeInteger ;
                ] .

paml:Duration rdf:type owl:Class ;
                rdfs:comment "An Activity duration is a TimeVariable" ;
                rdfs:subClassOf sbol:TopLevel ,
                [ rdf:type owl:Restriction ;
                  owl:onProperty paml:timeOf ;
                  owl:minCardinality "1"^^xsd:nonNegativeInteger ;
                  owl:maxCardinality "1"^^xsd:nonNegativeInteger ;
                ] ,
                [ rdf:type owl:Restriction ;
                      owl:onProperty paml:timeVariableValue ;
                      owl:onClass sbol:Measure ;
                      owl:minCardinality "0"^^xsd:nonNegativeInteger ;
                      owl:maxCardinality "1"^^xsd:nonNegativeInteger ;
                ] .


paml:Protocol rdfs:subClassOf
                 [ rdf:type owl:Restriction ;
                   owl:onProperty paml:timeConstraints ;
                   owl:onClass paml:TimeVariable,
                               paml:Duration ;
                 ] .

paml:timeConstraints rdf:type owl:ObjectProperty ;
              rdfs:domain paml:Protocol ;
              rdfs:range paml:TimeVariable,
                         paml:Duration ;
              rdfs:label "time_constraints" ;
              rdfs:comment "Specification of time constraints on a protocol" .

# <expression> ::= <duration>| <float> | <float> * <expression> |  <expression> - <expression> | <expression> + <expression>

paml:unaryExpressionTerm rdf:type owl:ObjectProperty ;
              rdfs:domain paml:UnaryExpression ;
              rdfs:range paml:TimeVariable,
                         paml:Duration,
                         sbol:Measure ;
              rdfs:label "term" ;
              rdfs:comment "Expression Term" .

paml:binaryExpressionTerm1 rdf:type owl:ObjectProperty ;
              rdfs:domain paml:BinaryExpression ;
              rdfs:range paml:Expression ;
              rdfs:label "term1" ;
              rdfs:comment "Expression Term1" .

paml:binaryExpressionTerm2 rdf:type owl:ObjectProperty ;
              rdfs:domain paml:BinaryExpression ;
              rdfs:range paml:Expression ;
              rdfs:label "term2" ;
              rdfs:comment "Expression Term2" .


paml:Expression rdf:type owl:Class ;
                rdfs:comment "An Arithmetic Expression" ;
                rdfs:subClassOf sbol:TopLevel .

paml:UnaryExpression rdf:type owl:Class ;
                rdfs:comment "A Unary Arithmetic Expression" ;
                rdfs:subClassOf paml:Expression .

paml:BinaryExpression rdf:type owl:Class ;
                rdfs:comment "A Binary Arithmetic Expression" ;
                rdfs:subClassOf paml:Expression ,
                [ rdf:type owl:Restriction ;
                  owl:onProperty paml:binaryExpressionTerm1 ;
                  owl:onClass paml:Expression ;
                  owl:minCardinality "1"^^xsd:nonNegativeInteger ;
                  owl:maxCardinality "1"^^xsd:nonNegativeInteger ;
                ],
                [ rdf:type owl:Restriction ;
                  owl:onProperty paml:binaryExpressionTerm2 ;
                  owl:onClass paml:Expression ;
                  owl:minCardinality "1"^^xsd:nonNegativeInteger ;
                  owl:maxCardinality "1"^^xsd:nonNegativeInteger ;
                ]    .


paml:VariableExpression rdf:type owl:Class ;
                rdfs:comment "A Variable Expression" ;
                rdfs:subClassOf paml:UnaryExpression ,
                [ rdf:type owl:Restriction ;
                  owl:onProperty paml:unaryExpressionTerm ;
                  owl:onClass paml:TimeVariable,
                              paml:Duration;
                  owl:minCardinality "1"^^xsd:nonNegativeInteger ;
                  owl:maxCardinality "1"^^xsd:nonNegativeInteger ;
                ]  .

paml:ConstantExpression rdf:type owl:Class ;
                rdfs:comment "A Constant Expression" ;
                rdfs:subClassOf paml:UnaryExpression ,
                [ rdf:type owl:Restriction ;
                  owl:onProperty paml:unaryExpressionTerm ;
                  owl:onClass sbol:Measure ;
                  owl:minCardinality "1"^^xsd:nonNegativeInteger ;
                  owl:maxCardinality "1"^^xsd:nonNegativeInteger ;
                ]   .

paml:ProductExpression rdf:type owl:Class ;
                rdfs:comment "A Product Expression, where both terms are Expressions or constants." ;
                rdfs:subClassOf paml:BinaryExpression .

paml:DifferenceExpression rdf:type owl:Class ;
                rdfs:comment "A  Difference Expression, where both terms are  Expressions or constants." ;
                rdfs:subClassOf paml:BinaryExpression .

paml:SumExpression rdf:type owl:Class ;
                rdfs:comment "A  Difference Expression, where both terms are  Expressions or constants." ;
                rdfs:subClassOf paml:BinaryExpression .

# <bincomp> ::= <= | < | >= | > | =

paml:binaryComparisonTerm1 rdf:type owl:ObjectProperty ;
              rdfs:domain paml:BinaryComparison ;
              rdfs:range paml:Expression ;
              rdfs:label "term1" ;
              rdfs:comment "Comparison Term1" .

paml:binaryExpressionTerm2 rdf:type owl:ObjectProperty ;
              rdfs:domain paml:BinaryComparison ;
              rdfs:range paml:Expression ;
              rdfs:label "term2" ;
              rdfs:comment "Comparison Term2" .

paml:BinaryComparison rdf:type owl:Class ;
                rdfs:comment "Binary Comparison Abstract Class" ;
                rdfs:subClassOf sbol:TopLevel ,
                [ rdf:type owl:Restriction ;
                  owl:onProperty paml:binaryComparisonTerm1 ;
                  owl:onClass paml:Expression ;
                  owl:minCardinality "1"^^xsd:nonNegativeInteger ;
                  owl:maxCardinality "1"^^xsd:nonNegativeInteger ;
                ],
                [ rdf:type owl:Restriction ;
                  owl:onProperty paml:binaryComparisonTerm2 ;
                  owl:onClass paml:Expression ;
                  owl:minCardinality "1"^^xsd:nonNegativeInteger ;
                  owl:maxCardinality "1"^^xsd:nonNegativeInteger ;
                ]

paml:LessThanComparison rdf:type owl:Class ;
                rdfs:comment "Less Than Comparison Class" ;
                rdfs:subClassOf paml:BinaryComparison .

paml:LessThanEqualComparison rdf:type owl:Class ;
                rdfs:comment "Less Than Equal Comparison Class" ;
                rdfs:subClassOf paml:BinaryComparison .

paml:GreaterThanEqualComparison rdf:type owl:Class ;
                rdfs:comment "Greater Than Equal Comparison Class" ;
                rdfs:subClassOf paml:BinaryComparison .

paml:GreaterThanEqualComparison rdf:type owl:Class ;
                rdfs:comment "Greater Than Equal Comparison Class" ;
                rdfs:subClassOf paml:BinaryComparison .

paml:EqualComparison rdf:type owl:Class ;
                rdfs:comment "Equal Comparison Class" ;
                rdfs:subClassOf paml:BinaryComparison .


# <binlog> ::= AND | OR | XOR
# <constraint> ::= <expression> <bincomp> <expression> | <constraint> <binlog> <constraint> | NOT <constraint>

paml:Constraint rdf:type owl:Class ;
                rdfs:comment "Abstract Constraint Class" ;
                rdfs:subClassOf sbol:TopLevel .